{
    "name": "weather-fetcher",
    "version": "2.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "start": "concurrently --raw --kill-others \"yarn dev-server\" \"yarn tsm\"",
        "dev-server": "webpack-dev-server --config=./webpack/webpack.config.js --mode development --open --hot",
        "deploy": "npm ci && npm run prod",
        "prod": "webpack --config=./webpack/webpack.config.js --mode production && npm run build-storybook",
        "analyze": "source-map-explorer build/static/js/main.*",
        "lint": "eslint --fix . && echo 'Lint complete.'",
        "test": "jest",
        "jest-watch": "npm test -- --watch",
        "jest-ci": "npm test -- --ci",
        "cypress:open": "cypress open",
        "cypress:ci": "cypress run",
        "storybook": "start-storybook -p 6006",
        "build-storybook": "build-storybook -o ./dist/sb",
        "tsm": "tsm src --watch --exportType default --exportTypeName ClassesType --logLevel silent"
    },
    "repository": "git+https://github.com/shimizacken/weather-fetcher.git",
